<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:t="sap.ui.table">
	<Dialog title="Interest Details" class="sapUiContentPadding sapUiSizeCompact" contentWidth="100%" initialFocus="done">
		<content>
			<l:Grid defaultSpan="L12 M12 S12" vSpacing="0.5" hSpacing="0.5">
				<l:content>

						<Text text="Month"/>
						<Input id="idMnInp" type="Number"/>
							<Text text="Interest rate"/>
						<Input id="idRoiInp" type="Number"/>
						<HBox justifyContent="End">
							<Button text="Add" press="onAddROI"/>
						</HBox>
						
					<t:Table  rows="{iDialogModel>/}" enableColumnReordering="false" visibleRowCount="5" selectionMode="None" showNoData="true"
						rowActionCount="1" fixedColumnCount="1" enableCellFilter="true">
						<t:columns>
						
							<t:Column sortProperty="" filterProperty="" hAlign="Start" resizable="false" >
								<Text text="Month" class="white bold" textAlign="Begin" wrapping="false" width="100%"/>
								<t:template>
									<Text text="{iDialogModel>month}" press=".onNav(${oModel>})"/>
								</t:template>
							</t:Column>
						
							<t:Column sortProperty="" filterProperty="" hAlign="Start" resizable="false" >
								<Text text="Interest rate" class="white bold" textAlign="Begin" wrapping="false" width="100%"/>
								<t:template>
									<Text text="{iDialogModel>roi}"/>
								</t:template>
							</t:Column>
							
						
						</t:columns>
						<t:rowActionTemplate> 
							<t:RowAction> 
							 	<t:RowActionItem icon="sap-icon://delete" text="{i18n>delete}" press="onDelIntMonth" visible="{=(${iDialogModel>month}!==1&amp;&amp;${iDialogModel>month}!=='1')}"/>
						 	</t:RowAction>
						</t:rowActionTemplate>
					</t:Table>
				</l:content>
			</l:Grid>
		</content>
		<buttons>
			<Button type="Ghost" text="Close" press="onClose" class="sapUiTinyMarginEnd"/>
			<Button type="Accept" text="Update" press="cUpdateInt" class="sapUiTinyMarginEnd"/>
		</buttons>
		<beginButton></beginButton>
		<endButton></endButton>
	</Dialog>
</core:FragmentDefinition>
